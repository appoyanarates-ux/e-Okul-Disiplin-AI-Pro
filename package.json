{
  "name": "e-okul-disiplin-ai-pro",
  "version": "1.0.9",
  "description": "E-Okul Disiplin AI Pro, eğitim kurumlarında disiplin süreçlerinin düzenli, hızlı ve güvenli bir şekilde yürütülmesini sağlayan profesyonel masaüstü uygulamasıdır.",
  "private": true,
  "type": "module",
  "main": "main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "electron": "electron .",
    "dist": "npm run build && electron-builder"
  },
  "build": {
    "appId": "com.okul.disiplin.ai.pro",
    "productName": "e-Okul Disiplin AI Pro",
    "files": [
      "dist/**/*",
      "main.js",
      "package.json"
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "appoyanarates-ux",
        "repo": "e-Okul-Disiplin-AI-Pro"
      }
    ],
    "win": {
      "target": "nsis"
    }
  },
  "dependencies": {
    "@google/genai": "^1.40.0",
    "electron-updater": "^6.7.3",
    "html2canvas": "^1.4.1",
    "jspdf": "^4.1.0",
    "lucide-react": "^0.563.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-is": "^19.2.4",
    "react-router-dom": "^7.13.0",
    "recharts": "^3.7.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^25.3.1",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.24",
    "electron": "^40.1.0",
    "electron-builder": "^26.4.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "vite": "^6.2.0"
  }
}